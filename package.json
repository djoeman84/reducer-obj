{
  "name": "reducer-obj",
  "version": "0.0.3",
  "license": "MIT",
  "description": "A package to allow you to write reducers as objects",
  "keywords": [
    "redux",
    "object",
    "called",
    "reducer-obj"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/djoeman84/reducer-obj.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src -d dist",
